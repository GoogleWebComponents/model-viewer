{
  "name": "@google/3dom",
  "version": "0.0.1",
  "description": "An isolated scene graph facade for web-based 3D libraries",
  "repository": "https://github.com/google/model-viewer",
  "bugs": {
    "url": "https://github.com/google/model-viewer/issues"
  },
  "homepage": "https://github.com/google/model-viewer#readme",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "author": "DevXR constributors",
  "main": "./lib/api.js",
  "module": "./lib/api.js",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "test": "karma start --single-run",
    "test:ci": "npm run test",
    "test:watch": "karma start",
    "build": "tsc",
    "watch:tsc": "tsc -w",
    "clean": "rm -rf ./lib",
    "update:package-lock": "rm ./package-lock.json && npm i --only=production",
    "dev": "tsc && npm-run-all --parallel 'watch:tsc -- --preserveWatchOutput' 'watch:rollup' serve"
  },
  "keywords": [
    "gltf",
    "glb",
    "webxr",
    "augmented reality",
    "3d"
  ],
  "devDependencies": {
    "@jsantell/event-target": "^1.1.2",
    "@open-wc/karma-esm": "^2.11.1",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.1.2",
    "karma": "^4.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sauce-launcher": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "mocha": "^5.2.0",
    "polymer-build": "^3.1.1",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "three": "^0.113.1"
  },
  "publishConfig": {
    "access": "public"
  }
}